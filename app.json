{
  "expo": {
    "name": "TeachMe",
    "slug": "TeachMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/TeachMeLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/TeachMeLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "NSFaceIDUsageDescription":"ensure only intended user is making changes to settings"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/TeachMeLogo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/TeachMeLogo.png"
    },
    "infoPlist" : {
      "NSFaceIDUsageDescription":"ensure only intended user is making changes to settings"
    }
  }
}
